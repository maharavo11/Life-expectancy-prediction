# Trace plot.
idata = az.from_cmdstanpy(fit)
az.plot_trace(idata, var_names=['theta_0', 'theta_1', 'theta_2', 'theta_3', 'theta_4', 'sigma'], 
              compact = False, combined = True, figsize=(16, 22))

# Posterior samples.
az.plot_posterior(idata, var_names=['theta_0', 'theta_1', 'theta_2', 'theta_3', 'theta_4', 'sigma'])
